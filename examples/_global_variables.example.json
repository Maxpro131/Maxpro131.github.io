  "$dé_QC:show_quick_move_items_inv": true, //Quick Move/Drop Bulk Items on Inventory
  "$dé_QC:multiple_craft_button_amount": 4,
  
//- Settings Screen
  "$dé:enable_force_spectator": true,
  
//- Tools Section
  //- Minimap Renderer
    "$dé:include_entities": true,
    // Rendering area
    "$dé:bottom_left_corner": [-128, -60, -128],
    "$dé:top_right_corner": [128, 316, 128],
  
//- Other In-Game Configuration
  "$dé:force_render_below": false, //render hud screen when overlapping with other screens

  //- Trade Screen
  "$dé:enable_trade_unlocker": true,
  
  //- Item ID Offset
    // Enable Item ID Offset Editor in game.
    "$dé:enable_item_id_offset_editor": false,
