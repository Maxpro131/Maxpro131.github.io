{
  "pageName": "Déesse UI — Editor",
  "variables": {
    "$déesse_ui_global_variables_version": {
      "type": "number",
      "input": "text",
      "readonly": true,
      "label": "Config version",
      "help": "",
      "default": "3"
    },
    "$dé:show_inventory_full_notification": {
      "type": "boolean",
      "input": "toggle",
      "readonly": false,
      "label": "Inventory full notification",
      "help": "Enable or disable the \"Inventory full\" notification",
      "default": true
    },
    "$dé:inv_full_notification_duration": {
      "type": "number",
      "input": "slider",
      "min": 0.5,
      "max": 10,
      "step": 0.5,
      "readonly": false,
      "label": "Inventory full notification duration",
      "help": "",
      "default": 3
    },
    "$dé:show_world_height_meter": {
      "type": "boolean",
      "input": "toggle",
      "readonly": false,
      "label": "",
      "help": "",
      "default": false
    },
    "$dé:show_item_durability_low_warning": {
      "type": "boolean",
      "input": "toggle",
      "readonly": false,
      "label": "Durability low warning",
      "help": "",
      "default": true
    },
    "$dé:enable_center_item_stack": {
      "type": "boolean",
      "input": "toggle",
      "readonly": false,
      "label": "",
      "help": "",
      "default": false
    },
    "$dé:use_bottom_chat": {
      "type": "boolean",
      "input": "toggle",
      "readonly": false,
      "label": "If the chat should be at the bottom",
      "help": "Disable this option for the chat to be at the top",
      "default": true
    },
    "$dé:show_progress_time_chat": {
      "type": "boolean",
      "input": "toggle",
      "readonly": false,
      "label": "Shows progress time until a chat message disappears",
      "help": "",
      "default": true
    },
    "$dé:max_chat_item": {
      "type": "number",
      "input": "slider",
      "min": 10,
      "max": 100,
      "step": 1,
      "readonly": false,
      "label": "Maximum messages shown in chat",
      "help": "Do not use decimal numbers",
      "default": 50
    },
    "$dé:player_list_header_bg_alpha": {
      "type": "number",
